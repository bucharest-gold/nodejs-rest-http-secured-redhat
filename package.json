{
  "name": "nodejs-rest-http-secured-redhat",
  "version": "1.1.2",
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "lint": "xo",
    "prepare": "echo 'To confirm CVE compliance, run \"npm run security-check\"' ",
    "security-check": "nsp check",
    "coverage": "nyc npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "ci": "npm run lint && npm run coveralls",
    "dependencyCheck": "szero . --ci",
    "release": "standard-version -a",
    "openshift": "nodeshift --strictSSL=false --dockerImage=registry.access.redhat.com/rhoar-nodejs/nodejs-8",
    "postinstall": "license-reporter report -s && license-reporter save -s --xml licenses.xml",
    "start": "node ."
  },
  "main": "./bin/www",
  "standard-version": {
    "scripts": {
      "postbump": "npm run postinstall",
      "precommit": "git add licenses/"
    }
  },
  "xo": {
    "space": 2,
    "ignores": [
      "public/**"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        "always"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bucharest-gold/nodejs-rest-http-secured-redhat.git"
  },
  "files": [
    "package.json",
    "app.js",
    "public",
    "bin",
    "keycloak.json",
    "kc.json",
    "LICENSE",
    "licenses"
  ],
  "bugs": {
    "url": "https://github.com/bucharest-gold/nodejs-rest-http-secured-redhat/issues"
  },
  "homepage": "https://github.com/bucharest-gold/nodejs-rest-http-secured-redhat",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "nodeshift": "~1.11.0",
    "nsp": "~3.2.1",
    "nyc": "~12.0.2",
    "proxyquire": "^2.0.0",
    "standard-version": "^4.3.0",
    "supertest": "^3.0.0",
    "szero": "^1.0.0",
    "tap-spec": "~5.0.0",
    "tape": "~4.9.0",
    "xo": "~0.22.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "debug": "^3.1.0",
    "express": "4.16.0",
    "keycloak-connect": "^4.1.0",
    "kube-probe": "^0.3.0",
    "license-reporter": "^1.2.0"
  }
}
