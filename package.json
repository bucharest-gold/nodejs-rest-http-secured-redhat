{
  "name": "nodejs-rest-http-secured-redhat",
  "version": "2.0.2",
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "lint": "xo",
    "coverage": "nyc npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "ci": "npm run lint && npm run coveralls",
    "release": "standard-version -a",
    "openshift": "nodeshift --strictSSL=false --dockerImage=registry.access.redhat.com/rhoar-nodejs/nodejs-10",
    "postinstall": "license-reporter report -s && license-reporter save -s --xml licenses.xml",
    "start": "node ."
  },
  "main": "./bin/www",
  "standard-version": {
    "scripts": {
      "postbump": "npm run postinstall",
      "precommit": "git add licenses/"
    }
  },
  "xo": {
    "space": 2,
    "ignores": [
      "public/**"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        "always"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nodeshift-starters/nodejs-rest-http-secured-redhat.git"
  },
  "files": [
    "package.json",
    "app.js",
    "public",
    "bin",
    "keycloak.json",
    "kc.json",
    "LICENSE",
    "licenses"
  ],
  "bugs": {
    "url": "https://github.com/nodeshift-starters/nodejs-rest-http-secured-redhat/issues"
  },
  "homepage": "https://github.com/nodeshift-starters/nodejs-rest-http-secured-redhat",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "nodeshift": "~4.0.0",
    "nyc": "~14.1.1",
    "proxyquire": "~2.1.3",
    "standard-version": "^6.0.1",
    "supertest": "~4.0.2",
    "tap-spec": "~5.0.0",
    "tape": "~4.11.0",
    "xo": "~0.24.0"
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "debug": "^4.0.1",
    "express": "~4.17.1",
    "keycloak-connect": "^6.0.1",
    "kube-probe": "~0.3.2",
    "license-reporter": "~1.2.2"
  }
}
